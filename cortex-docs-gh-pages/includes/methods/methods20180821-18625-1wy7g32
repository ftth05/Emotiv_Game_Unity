      <tr>
        <th>Parameter</th>
        <th>Type</th>
        <th>Required</th>
        <th>Description</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>_auth</td>
        <td>string (<a href="#auth-token">Auth Token</a>)</td>
        <td>Yes</td>
        <td>Auth token</td>
      </tr>
      <tr>
        <td>session</td>
        <td>string</td>
        <td>No</td>
