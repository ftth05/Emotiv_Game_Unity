      <tr>
        <td>currentThreshold</td>
        <td>number</td>
        <td> </td>
      </tr>
      <tr>
        <td>lastTrainingScore</td>
        <td>number</td>
        <td> </td>
      </tr>
    </tbody>
  </table>

</div>

